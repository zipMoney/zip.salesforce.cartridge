<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">
    <type-extension type-id="SitePreferences">
        <custom-attribute-definitions>
			<attribute-definition attribute-id="zipLightbox">
                <display-name xml:lang="x-default">Lightbox</display-name>
                <description xml:lang="x-default">If turned on, ZIP will open in lightbox; false - ZIP will simply redirect to URL in the same page.</description>
                <type>boolean</type>
                <mandatory-flag>true</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>true</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="zipApiMode">
                <display-name xml:lang="x-default">Zip API Mode Production</display-name>
                <description xml:lang="x-default">API Mode Production for Zip</description>
                <type>boolean</type>
                <mandatory-flag>true</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>false</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="zipTokenization">
                <display-name xml:lang="x-default">Zip Tokenization</display-name>
                <description xml:lang="x-default">Enable/disable tokenization for this processor</description>
                <type>boolean</type>
                <mandatory-flag>true</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>false</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="zipAutoCapture">
                <display-name xml:lang="x-default">Automatic Capture</display-name>
                <description xml:lang="x-default">Enable/disable automatic captures of charges for this processor.</description>
                <type>boolean</type>
                <mandatory-flag>true</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>true</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="zipMarketingEnvironment">
                <display-name xml:lang="x-default">Zip Marketing Environment</display-name>
                <description xml:lang="x-default">Environment for Zip Marketing (sandbox, production)</description>
                <type>enum-of-string</type>
                <mandatory-flag>true</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <value-definitions>
                    <value-definition default="true">
                        <display xml:lang="x-default">Sandbox</display>
                        <value>sandbox</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">Production</display>
                        <value>production</value>
                    </value-definition>
                </value-definitions>
            </attribute-definition>
            <attribute-definition attribute-id="zipMarketingKey">
                <display-name xml:lang="x-default">Zip Marketing Key</display-name>
                <description xml:lang="x-default">Marketing Key to activate Zip widgets.</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="zipApiMode">
                <display-name xml:lang="x-default">Zip API Mode Production</display-name>
                <description xml:lang="x-default">API Mode Production for Zip</description>
                <type>boolean</type>
                <mandatory-flag>true</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>false</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="zipApiKey">
                <display-name xml:lang="x-default">Zip API Key</display-name>
                <description xml:lang="x-default">API Key for Zip</description>
                <type>string</type>
                <mandatory-flag>true</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="zipTokenization">
                <display-name xml:lang="x-default">Zip Tokenization</display-name>
                <description xml:lang="x-default">Enable/disable tokenization for this processor</description>
                <type>boolean</type>
                <mandatory-flag>true</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>false</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="zipAutoCapture">
                <display-name xml:lang="x-default">Automatic Capture</display-name>
                <description xml:lang="x-default">Enable/disable automatic captures of charges for this processor.</description>
                <type>boolean</type>
                <mandatory-flag>true</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>true</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="zipPendingMessageStyles">
                <display-name xml:lang="x-default">Zip Pending Message Styles</display-name>
                <description xml:lang="x-default">Custom CSS styles for Zip Confirmation Message appearing on Thank You Page after placing an order with pending referral application.</description>
                <type>text</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>color: #6a0000; background-color: #f5cccc; border-color: #f1b8b8; font-size: 16px;</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="zipStorefrontCartridge">
                <display-name xml:lang="x-default">Storefront Cartridge</display-name>
                <description xml:lang="x-default">Name of storefront cartridge</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>app_storefront_controllers</default-value>
            </attribute-definition>
		</custom-attribute-definitions>
		<group-definitions>
			<attribute-group group-id="Zip">
                <display-name xml:lang="x-default">Zip</display-name>
				<attribute attribute-id="zipLightbox"/>
				<attribute attribute-id="zipTokenization"/>
                <attribute attribute-id="zipApiMode"/>
				<attribute attribute-id="zipApiKey"/>
				<attribute attribute-id="zipAutoCapture"/>
                <attribute attribute-id="zipMarketingEnvironment"/>
				<attribute attribute-id="zipMarketingKey"/>
                <attribute attribute-id="zipPendingMessageStyles"/>
            </attribute-group>
            <attribute-group group-id="ZipSG">
                <display-name xml:lang="x-default">Zip - Site Genesis</display-name>
				<attribute attribute-id="zipStorefrontCartridge"/>
            </attribute-group>
        </group-definitions>
	</type-extension>
